#include <iostream>
using namespace std;
long long d[201][201];
long long mod = 1000000000;
int main() {
    int n, k;
    cin >> n >> k;
    d[0][0] = 1LL; 

    for (int i=1; i<=k; i++) {
        for (int j=0; j<=n; j++) {
            for (int l=0; l<=j; l++) {
                d[i][j] += d[i-1][j-l];
                d[i][j] %= mod;
				// cout << i << "와 "<< j << "와 " << l << "은 = "<< d[i][j] << "\n";
            }
        }
    }
    cout << d[k][n] << '\n';
    return 0;
}
